{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://olivegardens-backend-production-d1a4.up.railway.app/api/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "https://olivegardens-backend-production-d1a4.up.railway.app/uploads/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}